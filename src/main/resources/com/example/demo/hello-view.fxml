<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="515.0" prefWidth="680.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.LineChartSample">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <BarChart fx:id="barChart">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </BarChart>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="9.0" prefWidth="551.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="148.0" text="Button" />
            <Button layoutX="175.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="148.0" text="Button" />
            <Button alignment="BOTTOM_CENTER" layoutX="346.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="148.0" text="Button" />
         </children>
      </AnchorPane>
   </children>
</VBox>
